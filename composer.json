{
    "name": "sadanand1991/rt-image-slideshow",
    "description": "RT Slideshow",
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "phpcompatibility/php-compatibility": "^9.3",
        "wp-coding-standards/wpcs": "^3.0"
    },
    "scripts": {
        "cs-check": "phpcs --standard=WordPress --ignore=vendor --extensions=php .",
        "cs-fix": "phpcbf --standard=WordPress --ignore=vendor --extensions=php ."
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
